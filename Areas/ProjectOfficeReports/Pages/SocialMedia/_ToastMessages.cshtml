@model IReadOnlyList<ProjectManagement.Areas.ProjectOfficeReports.SocialMedia.ViewModels.SocialMediaToastMessage>

@if (Model.Count > 0)
{
    <div class="visually-hidden" data-social-toast-container>
        @foreach (var toast in Model)
        {
            <span data-social-toast data-variant="@toast.Variant">@toast.Message</span>
        }
    </div>
}
